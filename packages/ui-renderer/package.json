{
  "name": "@fuzzy-next/ui-renderer",
  "version": "2.0.4",
  "description": "",
  "keywords": [
    "fuzzy-next",
    "ui"
  ],
  "license": "ISC",
  "author": "younghero",
  "files": [
    "dist",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./dist/ui-renderer.es.js",
      "require": "./dist/ui-renderer.umd.js"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "./lib/packages/ui-renderer",
  "main": "./dist/@fuzzy-next/ui-renderer.umd.js",
  "module": "./dist/@fuzzy-next/ui-renderer.es.js",
  "typings": "./lib/src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "vite build && tsc -p ./tsconfig.build.json",
    "pub": "pnpm run build && pnpm version patch && pnpm publish --access public --force --no-git-checks"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@fuzzy-next/ui-renderer": "^2.0.1",
    "fuzzy-next": "workspace:^1.0.52"
  },
  "devDependencies": {
    "tailwindcss": "^3.1.8"
  }
